{
  "name": "enjoy-socks",
  "version": "0.1.0",
  "description": "Online store of socks with the ability to edit the design.",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/app.js",
    "dev": "nodemon src/app.js --ext js,jsx,json --ignore sessions/",
    "db:up": "npx sequelize db:create; npx sequelize db:migrate; npx sequelize db:seed:all;",
    "db:down": "npx sequelize db:migrate:undo:all; npx sequelize db:drop;",
    "kill": "killall node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hristy-A/enjoy-socks.git"
  },
